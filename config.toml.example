# config.toml example
[[gateway]]

    [gateway.irc]
    # Listen address
    ListenAddr = ":6667"

    # Message of the Day
    MOTD = """
    haha !sux sonzai x"""

    [gateway.slack]
    # Slack client token
    token = ""
    # Slack client cookie (d)
    cookie = ""

# Multiple gateway sections can be specified for multiple workspaces
[[gateway]]
    [gateway.irc]
    ListenAddr = ":6668"

    [gateway.slack]
    token = ""